{
  "name": "SimpleTrader",
  "version": "1.4.0RC-2",
  "description": "Uma Simples Aplicação Para Acompanhar Moedas Digitais",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-osx": "electron-packager . SimpleTrader --platform=darwin --arch=x64 && cp imgs/SimpleTraderIcon.icns SimpleTrader-darwin-x64/SimpleTrader.app/Contents/Resources/electron.icns",
    "build-mas": "electron-packager . SimpleTrader --platform=mas --arch=x64 && cp imgs/SimpleTraderIcon.icns SimpleTrader-mas-x64/SimpleTrader.app/Contents/Resources/electron.icns",
    "build-win": "electron-packager . SimpleTrader --platform=win32 --arch=ia32 --icon=imgs/SimpleTraderIcon.icns --prune=true --version-string.CompanyName=Arkd --version-string.FileDescription=CE --version-string.ProductName='SimpleTrader'"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron"
  ],
  "author": "Davi Bispo",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^8.7.2"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-carousel": "^1.1.0",
    "angular-chart.js": "^1.1.1",
    "angular-route": "^1.6.5",
    "angular-sqlite": "^0.1.0",
    "angular-touch": "^1.6.6",
    "chart.js": "^2.6.0",
    "jquery": "^3.2.1",
    "materialize-css": "^0.99.0",
    "menubar": "*"
  }
}
